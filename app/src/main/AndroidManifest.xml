<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.easylife.aunweshaacademy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.easylife.aunweshaacademy.screens.Menu"></activity>
        <activity android:name="com.easylife.aunweshaacademy.screens.Browser"></activity>
        <activity android:name="com.easylife.aunweshaacademy.screens.Login" />
        <activity android:name="com.easylife.aunweshaacademy.screens.Register" />
        <activity android:name="com.easylife.aunweshaacademy.screens.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>